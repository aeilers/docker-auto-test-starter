{
  "name": "docker-auto-test-starter",
  "version": "1.0.0",
  "description": "Starter kit for js-auto-test implementations in Docker",
  "main": "gulpfile.babel.js",
  "scripts": {
    "build": "NODE_ENV=production gulp zip",
    "test": "NODE_ENV=production xvfb-run --server-args='-screen 0 1280x1024x24' chromedriver --port=9515 --log-path=/dev/stdout & sleep 5 && ./node_modules/protractor/bin/protractor ./conf/config.js",
    "test:dev": "NODE_ENV=development xvfb-run --server-args='-screen 0 1280x1024x24' chromedriver --port=9515 --log-path=/dev/stdout & gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeilers/docker-auto-test-starter.git"
  },
  "bugs": {
    "url": "https://github.com/aeilers/docker-auto-test-starter/issues"
  },
  "homepage": "https://github.com/aeilers/docker-auto-test-starter#readme",
  "author": "Adam Eilers <adam.eilers@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "dependencies": {
    "js-auto-test": "~0.0.3-beta.2",
    "mocha": "~3.2.0",
    "protractor": "~5.1.1"
  },
  "devDependencies": {
    "babel-cli": "~6.24.1",
    "babel-core": "~6.24.1",
    "babel-eslint": "~7.2.1",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-transform-async-to-generator": "~6.24.1",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-es2015-destructuring": "~6.23.0",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-es2017": "~6.24.1",
    "del": "~2.2.2",
    "eslint": "~3.19.0",
    "gulp": "~3.9.1",
    "gulp-babel": "~6.1.2",
    "gulp-cached": "~1.1.1",
    "gulp-changed": "~2.0.0",
    "gulp-debug": "~3.1.0",
    "gulp-eslint": "~3.0.1",
    "gulp-if": "~2.0.2",
    "gulp-load-plugins": "~1.5.0",
    "gulp-remember": "~0.3.1",
    "gulp-sourcemaps": "~2.6.0",
    "gulp-uglify": "~2.1.2",
    "gulp-util": "~3.0.8",
    "jshint-stylish": "~2.2.1"
  }
}
